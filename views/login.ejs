<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ("./partials/head") %>
</head>
<body>
    <%- include ("./partials/nav") %>
    <div class="login-container">
        <div class="login-text">
            <div><br>로그인</div>
        </div>

        <form action="login" method="POST">
            <div id="forms">
                <!--email-->
                <div class="row form email">
                    <div class="col s12" id="inputbox">
                        <label for="email">이메일</label>
                        <input class="textinput" type='email' id = "email">
                    </div>
                </div>

                <!--password-->
                <div class="row form password">
                    <div class="col s12" id="inputbox">
                        <label for="password">비밀번호</label>
                        <input class="textinput" type='password' id = "password">
                    </div>
                </div>

                <!--login button-->
                <div class="row form">
                    <div class="col s12">
                        <button class= "waves-effect waves-light btn" id="loginbtn" type="submit" value="로그인">로그인</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</body>
</html>